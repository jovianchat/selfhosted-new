		"@types/eslint": "^9.6.0",
		"@types/node": "^22.7.7",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.13",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
